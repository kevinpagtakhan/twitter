<h1>Tweets#personal_index</h1>
<p>Find me in app/views/tweets/personal_index.html.erb</p>

<%= render 'form' %>

<hr>

<% for tweet in @tweets do %>
  <p>
      <%= link_to "@" + tweet.user.handle, user_path(tweet.user.handle) %> wrote <%= time_ago_in_words tweet.created_at %> ago <br > 
  </p>
  <h3>
      <%= tweet.content %> <br >
  </h3>
  <br>

  <% end %>
