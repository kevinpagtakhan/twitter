<h1>Tweets#index</h1>
<p>Find me in app/views/tweets/index.html.erb</p>

<%= render 'form' %>

<% for tweet in @tweets do %>
  <p>
    <%= link_to "@" + tweet.user.handle, user_path(tweet.user.handle) %> <%= time_ago_in_words tweet.created_at %>
  </p>
  <p>
    <%= link_to tweet.content, tweet %>
  </p>
<% end %>
